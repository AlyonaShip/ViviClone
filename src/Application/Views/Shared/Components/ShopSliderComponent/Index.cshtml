@model List<Application.EntitiesModels.Models.SliderModel>
@using Application.EntitiesModels.Entities

<div class="main-content">
    <div class="banner banner-home banner-home3">
        <div class="container">
            <div class="supermartket-owl-carousel banner-slide" data-autoplaytimeout="3000" data-loop="yes" data-autoplay="yes" data-number="1" data-margin="0" data-navcontrol="yes" data-dots="yes">
                @for (int i = 0; i < Model.Count; i++)
                {
                    if (Model[i].Type == TypeSlider.Shop)
                    {
                        <div class="banner-home3-content slide-h3">
                            <figure>
                                <a href="@Model[i].LinkToWare" target="_blank">
                                    <img src="@Model[i].Base64Image" alt="banner home3" width="1170" height="495">
                                </a>
                            </figure>
                        </div>
                    }
                }
            </div>
        </div>
    </div>
</div>
